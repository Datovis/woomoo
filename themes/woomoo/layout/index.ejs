layout: false
---
<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <title><%= config.title %></title>
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  <!-- Apple -->
  <meta name="apple-mobile-web-app-capable" content="yes" />
  <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
  <!-- SEO -->
  <meta name="description" content="<%= config.description %>" />
  <%- open_graph({
    type: 'website',
    image: theme.thumb,
    title: theme.title,
    description: theme.description,
    twitter_id: theme.twitter,
    google_plus: theme.google_plus,
    fb_admins: 1303233785}) %>
  <meta name="twitter:site" content="@<%= theme.twitter %>">
  <meta name="twitter:title" content="<%= config.title %>">
  <meta name="twitter:description" content="<%= config.description %>">
  <meta name="twitter:image:src" content="<%= theme.thumb %>">
  <%- css('css/index/style') %>
  <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
  <!--[if lt IE 9]><script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
  <%- partial('_partial/google-analytics') %>
</head>
<body>
  <div id="wrap">
    <%- partial('_screen/intro') %>
    <%- partial('_screen/team') %>
    <%- partial('_screen/products') %>
    <%- partial('_screen/contact') %>
    <div id="indicator">
      <a href="#intro" id="indicator-intro" class="indicator-link">
        <i class="indicator-background"></i>
        <span class="indicator-text">Intro</span>
      </a>
      <a href="#team" id="indicator-team" class="indicator-link">
        <i class="indicator-background"></i>
        <span class="indicator-text">Team</span>
      </a>
      <a href="#products" id="indicator-products" class="indicator-link">
        <i class="indicator-background"></i>
        <span class="indicator-text">Products</span>
      </a>
      <a href="#contact" id="indicator-contact" class="indicator-link">
        <i class="indicator-background"></i>
        <span class="indicator-text">Contact</span>
      </a>
    </div>
  </div>
  <div id="mobile-nav">
    <a href="#intro" id="mobile-nav-intro" class="mobile-nav-link">Intro</a>
    <a href="#team" id="mobile-nav-team" class="mobile-nav-link">Team</a>
    <a href="#products" id="mobile-nav-products" class="mobile-nav-link">Products</a>
    <a href="#contact" id="mobile-nav-contact" class="mobile-nav-link">Contact</a>
  </div>
  <script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
  <%- js('js/libs/konami', 'js/home') %>
</body>
</html>
